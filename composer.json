{
    "name": "brodatskiy/bitrix24-rest-api",
    "type": "project",
    "description": "",
    "keywords": [
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bitrix24/b24phpsdk": "^1.6",
        "symfony/var-dumper": "^7.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
